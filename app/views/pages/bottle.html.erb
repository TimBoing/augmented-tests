<a-scene embedded arjs>
  <a-marker preset="hiro">
    <a-entity
      position="0 0 0"
      scale="0.5 0.5 0.5"
      gltf-model="<%= cloudinary_url("bouteille_augmented_nct1ub.glb",resource_type: :raw) %>"
