  <a-entity light="type: point; intensity: 10; distance: 50; decay: 2"
    position="0 1 10"></a-entity>  </a-marker>
  <a-entity camera></a-entity>
  <a-entity light="type: point; intensity: 10; distance: 50; decay: 2"
    position="0 1 -10"></a-entity>  </a-marker>
  <a-entity camera></a-entity>
  <a-entity light="type: point; intensity: 10; distance: 50; decay: 2"
    position="10 1 0"></a-entity>  </a-marker>
  <a-entity camera></a-entity>
  <a-entity light="type: point; intensity: 10; distance: 50; decay: 2"
    position="-10 1 0"></a-entity>  </a-marker>
  <a-entity camera></a-entity>
</a-scene>

