<a-scene embedded arjs>
  <a-marker preset="hiro">
    <a-entity
      position="0 0 0"
      scale="0.6 0.6 0.6"
      gltf-model="<%= cloudinary_url("planet_qn0kzv.glb",resource_type: :raw) %>"
      rotation="0 0 0" animation="property: rotation; to: 0 360 0; loop: true; dur: 10000"
    ></a-entity>

  <a-entity light="type: point; intensity: 10; distance: 50; decay: 2"
    position="0 1 4"></a-entity>  </a-marker>
  <a-entity camera></a-entity>
  <a-entity light="type: point; intensity: 10; distance: 50; decay: 2"
    position="0 1 -4"></a-entity>  </a-marker>
  <a-entity camera></a-entity>
  <a-entity light="type: point; intensity: 10; distance: 50; decay: 2"
    position="4 1 0"></a-entity>  </a-marker>
  <a-entity camera></a-entity>
  <a-entity light="type: point; intensity: 10; distance: 50; decay: 2"
    position="-4 1 0"></a-entity>  </a-marker>
  <a-entity camera></a-entity>
</a-scene>


